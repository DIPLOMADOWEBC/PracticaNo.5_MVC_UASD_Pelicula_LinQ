@{ 
    ViewBag.Title = "Listado de Peliculas";
}
@model IEnumerable< Práctica_No._5.Models.Pelicula>

<table>
    <tr>
        <th>
            Titulo
        </th>
        <th></th>
        <th>
            Director
        </th>
        <th></th>
        <th>
            Autor Principal
        </th>
        <th></th>
        <th>
            Num. Autores
        </th>
        <th></th>
        <th>
            Duracion
        </th>
        <th></th>
        <th>
            Estreno
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
    <tr>
        <td>@Html.DisplayFor(modelItem => item.Titulo)</td>
        <th></th>
        <td>@Html.DisplayFor(modelItem => item.Director)</td>
        <th></th>
        <td>@Html.DisplayFor(modelItem => item.AutorPrincipal)</td>
        <th></th>
        <td>@Html.DisplayFor(modelItem => item.numAutores)</td>
        <th></th>
        <td>@Html.DisplayFor(modelItem => item.Duracion)</td>
        <th></th>
        <td>@Html.DisplayFor(modelItem => item.Estreno)</td>
        <th></th>
    </tr>
    }
</table>